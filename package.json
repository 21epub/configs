{
  "name": "@dxsixpc/configs",
  "version": "0.4.18",
  "description": "项目通用配置",
  "keywords": [
    "commitlint",
    "eslint",
    "prettier",
    "stylelint",
    "rollup",
    "tsconfig",
    "types"
  ],
  "homepage": "https://dxsixpc.github.io/configs",
  "repository": "https://github.com/dxsixpc/configs",
  "license": "MIT",
  "author": {
    "name": "dxsixpc",
    "email": "dxsixpc@gmail.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:rollup": "rollup -c rollup.config.mjs",
    "build:rollup:watch": "rollup -c -w rollup.config.mjs",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "dev:rollup": "pnpm run watch",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "eslint --cache --ext .js,.jsx,.mjs,.ts,.tsx src",
    "prepare": "husky install",
    "prepublishOnly": "father doctor",
    "publish": "pnpm run prepublishOnly && node scripts/publish.mjs",
    "start": "dumi dev"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "commitizen": "^4.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "inquirer": "^8.2.5",
    "postcss": ">=8.2.15 <9.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "stylelint": "^15.2.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-eslint": "^9.0.3",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "dumi": "^2.1.15",
    "father": "^4.1.6",
    "fs-extra": "^11.1.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup": "^3.19.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-serve": "^2.0.2",
    "shelljs": "^0.8.5",
    "styled-components": "^5.3.8",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
